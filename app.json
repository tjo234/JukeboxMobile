{
  "expo": {
    "name": "JukeboxMobile",
    "slug": "JukeboxMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#15131c"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "tabletImage": "./assets/images/wide.png",
        "resizeMode": "contain",
        "backgroundColor": "#15131c"
      },
      "bundleIdentifier": "com.tjo234.JukeboxMobile",
       "infoPlist": {
        "NSBonjourServices": ["_.mpd._tcp"],
        "NSLocalNetworkUsageDescription": "Allow Jukebox to discover MPD servers on the local network."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#15131c"
      },
      "package": "com.tjo234.JukeboxMobile",
       "permissions": [
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.CHANGE_WIFI_MULTICAST_STATE",
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "90dbf2ba-25eb-4c7c-ae95-4a7e17afdac3"
      }
    }
  }
}
